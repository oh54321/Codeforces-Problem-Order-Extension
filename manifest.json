{
    "name": "Codeforces Order Alerter",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://codeforces.com/contest/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html", 
        "default_title": "Solve"
    },
    "permissions":[
        "alarms"
    ]
    
}